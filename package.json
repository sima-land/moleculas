{
  "name": "@sima-land/moleculas",
  "version": "0.0.0",
  "repository": "ssh://git@github.com:sima-land/moleculas.git",
  "author": "www.sima-land.ru team",
  "license": "UNLICENSED",
  "private": false,
  "engines": {
    "node": ">=14.16.0"
  },
  "scripts": {
    "build:before": "rimraf build",
    "build:main": "tsc --project tsconfig.build.json",
    "build:after": "node .build/dist.js",
    "build": "yarn build:before && yarn build:main && yarn build:after",
    "type-check": "tsc -p . --noEmit",
    "test": "jest",
    "coverage": "jest --coverage",
    "test:ci": "yarn coverage --maxWorkers=2 --colors --reporters=jest-junit --reporters=default",
    "lint": "yarn eslint && yarn stylelint",
    "eslint": "eslint --cache src/ --ext .js,.jsx,.ts,.tsx",
    "stylelint": "stylelint 'src/**/*.scss'",
    "storybook": "start-storybook -p 6006 -s ./.storybook/public",
    "build-storybook": "build-storybook --no-dll --quiet --loglevel error -s .storybook/public -o .build-storybook/storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn coverage"
    }
  },
  "dependencies": {
    "@sima-land/ui-nucleons": "^37.3.0",
    "@sima-land/ui-quarks": "^1.2.2",
    "classnames": "^2.3.1",
    "react-svg": "^11.2.2",
    "react-transition-group": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@sima-land/linters": "^1.0.12",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-docs": "^6.2.9",
    "@storybook/addon-storysource": "^6.0.26",
    "@storybook/addon-viewport": "^6.2.9",
    "@storybook/react": "^6.0.26",
    "@svgr/webpack": "^5.5.0",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.169",
    "@types/react-dom": "^17.0.4",
    "@types/react-transition-group": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "copyfiles": "^2.4.1",
    "date-fns": "^2.16.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "lodash": "4.17.15",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "rimraf": "^3.0.2",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4"
  }
}
